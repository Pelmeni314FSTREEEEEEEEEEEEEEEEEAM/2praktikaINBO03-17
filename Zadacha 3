#include "stdafx.h"
#include <iostream>
#include <cmath>
using namespace std;
int main() 
{ 
double a=4.6,b=1.5, y=0, t=0;
if (a<=b) (y=(a-b)/(a+b)*(a+b)/(a*a-a*b+b*b)); else y=a+log(b*b);
if (y==b) (t=(2*y + pow(y*y-a,1.0/2))/(2*b-pow(a*a-y,1.0/2)));
else if (y<b) (t=sin(y)y+1.0/tan(a-b));
else if (y>b) (t=pow(y*sin(a),1/3)+1.0/(sqrt(y*cos(b))));
printf ("a=%.1f\n",a);
printf ("b=%.1f\n",b);
printf ("y=%.2f\n",y);
printf ("t=%.2f",t);
system("pause");
return 0;
}
