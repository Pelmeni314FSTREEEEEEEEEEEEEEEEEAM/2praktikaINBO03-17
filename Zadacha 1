#include "stdafx.h"
#include <iostream>
#include <cmath>
using namespace std;

int main() 
{ 
double a=4.8,b=-7.9,n=0,m=0;
if (a>=b) (n=pow(a-b,1.0/3));
else (n=a*a+(a-b)/sin(a*b));
if (n<b) (m=(n+a)/(-b)+sqrt(sin(a)*sin(a)-cos(n)));
else if (n==b) (m=b*b+tan(n*a)); else (m=b*b*b+n*a*a);
printf ("a=%.1f\n",a);
printf ("b=%.1f\n",b);
printf ("n=%.2f\n",n);
printf ("m=%.2f",m);
system("pause");
return 0;
}
